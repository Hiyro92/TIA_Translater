<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane minHeight="500.0" minWidth="500.0" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="634.0" prefWidth="894.0">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem fx:id="mButtonNew" mnemonicParsing="false" text="New Project" />
                        <MenuItem fx:id="mButtonOpen" mnemonicParsing="false" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                    <MenuItem fx:id="mButtonClose" mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Label fx:id="txtProjectName" alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="create or load a project">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <SplitPane fx:id="splitView" dividerPositions="0.5" orientation="VERTICAL" prefHeight="561.0" prefWidth="835.0" VBox.vgrow="ALWAYS">
              <items>
                  <TableView fx:id="table" fixedCellSize="2.0" />
                  <fx:include source="../Logger/LoggerConsole.fxml" />
              </items>
            </SplitPane>
         </children>
      </VBox>
   </children>
</StackPane>
