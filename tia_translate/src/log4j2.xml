<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <Console name="STDOUT" target="SYSTEM_OUT" follow="true">
        <PatternLayout pattern="%d %-5p (%F:%L) - %m%n" />
    </Console>
</Appenders>
<Loggers>
    <Root level="DEBUG">
        <AppenderRef ref="STDOUT" />
    </Root>
</Loggers>
</Configuration>